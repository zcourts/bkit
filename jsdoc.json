{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(.*\/node_modules\/.*|.*\/demo\/.*|^|\\/|\\\\)_",
        "include": ["bkit"],
        "exclude": []
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "systemName": "BKit",
        "footer": "BKit, the lighter, better widget toolkit",
        "copyright": "BKit Copyright Â© 2014",
        "navType": "inline",
        "theme": "journal",
        "linenums": true,
        "collapseSymbols": false,
        "inverseNav": true
    },
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    }
}
